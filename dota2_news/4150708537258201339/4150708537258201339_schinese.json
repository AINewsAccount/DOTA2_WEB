      "Lin \"Xxs\" Jing",
      "Zhao \"XinQ\" Zixing",
      "Ding \"Dy\" Cong",
      "[/expand]",
      "[*]Team Falcons",
      "[expand type=details]",
      "Oliver \"skiter\" Lepko",
      "Stanislav \"Malr1ne\" Potorak",
      "Ammar \"ATF\" Al-Assaf",
      "Andreas \"Cr1t-\" Nielsen",
      "Wu \"Sneyking\" Jingjun",
      "[/expand]",
      "[*]Team Liquid",
