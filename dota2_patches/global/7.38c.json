  "general_notes": [
    {
      "title": "Dire Safe Lane Jungle",
      "generic": [
        {
          "indent_level": 1,
          "note": "The Dire Safe Lane Tier 1 tower has been moved forward, away from the Dire base and closer to where the creeps initially meet"
        },
        {
          "indent_level": 1,
          "note": "The Top Lane creep paths have been slightly adjusted so the creeps meet closer to the Dire tower"
        },
        {
          "indent_level": 1,
          "note": "The tree lines above and below the tower have been reworked"
        },
        {
          "indent_level": 1,
          "note": "The Dire Safe Lane hard camp has been moved away from the Dire Safe Lane Tier 1 tower, and had its tree line reduced and rotated to face the river"
        },
        {
          "indent_level": 1,
          "note": "The cliff above the Dire Safe Lane small camp has been extended slightly towards the Dire Safe Lane Tier 1 tower, and the small camp has been moved and had its tree line adjusted"
        },
        {
          "indent_level": 1,
          "note": "The ramp leading to the river and the Roshan Pit from the Dire Safe Lane pull area has been moved towards the Pit slightly"
        },
        {
          "indent_level": 1,
          "note": "Several additional tree and visual adjustments have been made to this area"
        }
      ]
    },
    {
      "title": "Top Roshan Pit",
      "generic": [
        {
          "indent_level": 1,
          "note": "Fixed a spot at the north side of the pit that had incorrect vision rules"
        },
        {
          "indent_level": 1,
          "note": "Fixed some areas that visually looked pathable but were not"
        }
      ]
    },
    {
      "title": "Bottom Lane",
      "generic": [
        {
          "indent_level": 1,
          "note": "Trees have been added around the Dire Bottom Tier 1 tower, and several tree positions adjusted"
        },
        {
          "indent_level": 1,
          "note": "A tree near the Radiant Safe Lane hard camp has been removed"
        },
        {
          "indent_level": 1,
          "note": "The Watcher above the secret shop has been repositioned between the ramps from Dire Mid Tier 1 and the bottom Roshan Pit"
        },
        {
          "indent_level": 1,
          "note": "Fixed unnecessary pathing blockers at the left of the entrance to the Dire Base through the team only gate"
        },
        {
          "indent_level": 1,
          "note": "Several trees have been removed from the Radiant high ground leading to the Bottom Roshan Pit"
        },
        {
          "indent_level": 1,
          "note": "Several additional tree and visual adjustments have been made to this area"
        }
      ]
    }
  ],
