  "battlepass_caster_sound_pack_8": "Hagyományos eposzi hangcsomag",
  "battlepass_chat_wheel_boing": "Boing",
  "battlepass_chat_wheel_moo": "Múú",
  "battlepass_chat_wheel_orchestrahit": "Zenekari felütés",
  "battlepass_chat_wheel_heehaw": "Iáá",
