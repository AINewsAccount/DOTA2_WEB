  "ti11_immortal_1_9_desc": "Paket mit angepasstem Effekt für Sharpshooter sowie einer Verspottung",
  "ti11_immortal_1_10_name": "Das Schlachtenpassemblem 2022",
  "ti11_immortal_1_10_desc": "Globaler Gegenstand mit exklusivem Auraeffekt für den eigenen Helden",
  "ti11_immortal_1_11_name": "Das Schlachtenpassemblem 2022",
  "ti11_immortal_1_11_desc": "Globaler Gegenstand mit exklusivem Auraeffekt für den eigenen Helden",
  "ti11_immortals_1_levels_title": "Sie erhalten Schatz I auf den Leveln",
  "ti11_immortals_1_levels": "1 | 21 | 47 | 76 | 109 | 229 | 251",
  "ti11_immortals_1_levels_desc": "Nach Level 251 einmal Schatz I alle 30 Level",
  "ti11_immortal_2_1_name": "Glieder der Wolfsspinnen",
  "ti11_immortal_2_1_desc": "Beingegenstand mit angepasstem Effekt für Spin Web",
