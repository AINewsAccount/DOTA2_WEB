  "729_patch_map_change8_title": "Safe-Lane der Dire",
  "729_patch_map_change17_title": "Untere Ward-Platzierung der Dire",
  "729_patch_map_change9_title": "Oberer Stufe-2-Turm der Radiant",
  "729_patch_map_change10_title": "Mittlerer Stufe-2-Turm der Radiant",
  "729_patch_map_change11_title": "Unterer Stufe-2-Turm der Radiant",
  "729_patch_map_change12_title": "Oberer Stufe-2-Turm der Dire",
  "729_patch_map_change13_title": "Mittlerer Stufe-2-Turm der Dire",
  "729_patch_map_change14_title": "Unterer Stufe-2-Turm der Dire",
