  "battlepass_chat_wheel_boing": "Pružina",
  "battlepass_chat_wheel_moo": "Kráva",
  "battlepass_chat_wheel_orchestrahit": "Okamžik pravdy",
  "battlepass_chat_wheel_heehaw": "Oslík",
  "battlepass_chat_wheel_oops": "Ups",
  "battlepass_chat_wheel_woopwoo": "Juchů",
  "battlepass_chat_wheel_ceeeb": "Ceeeb",
  "battlepass_voice_announcer_prefix": "Balíček hlasatelů",
  "battlepass_voice_announcer_header": "Voice of The International",
  "battlepass_voice_announcer_desc": "Tento rok máte jedinečnou příležitost prodchnout všechny své zápasy atmosférou přímo ze šampionátu The International. Probojujte se na 75. úroveň Deníku a dostanete balíček „Voice of The International“ zajišťující hlasatele běžně známého z arén legendárního turnaje.",
  "battlepass_ranged_attack_header": "Speciální efekt útoků na dálku",
  "battlepass_ranged_attack_desc": "Zapomeňte na nudné efekty útoků na dálku. Představujeme letošní nádherně zlatý efekt, který můžete propůjčit všem svým hrdinům preferujícím útočení z odstupu.",
  "battlepass_taunts_header": "Taunty",
  "battlepass_taunt_1": "Curious Movements Indeed",
